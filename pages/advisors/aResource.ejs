<div class="row">
  <div class="span6 offset3">
            <h4>Documents:</h4>
            <div id="resourceList">
              <table class="table table-bordered">
                <thead>
                  <tr>
                    <th>Title</th>
                    <th>isDoc</th>
                  </tr>
                 <tbody>
		   <% for (var index in results) {
                      var result = results[index] %>
                        <tr class="apt-row">
                          <td><%= result.title %></td>
                          <td><%= result.isDoc %></td>
                        </tr>
                      <% } %>
                    </tbody>
                  </thead>
                </table>
             </div>
        </div>
    </div>
