
    <div class="row">
        <div class="span6 offset3">
            <h4>Upcoming appointments:</h4>
            <table id="adv-upcoming" class="table table-bordered">
                <thead>
                    <tr>
                        <th>Time</th>
                        <th>Name</th>
                    </tr>
                </thead>
                <tbody>
                    <% for (var abb in apts) {
                        var apt = apts[abb] %>
                    <tr>
                        <td id="<%= apt.netid %>"><%= apt.time %></td>
                        <td><%= apt.lname %>, <%= apt.fname %></td>
                    </tr>
                    <% } %>
                </tbody>
            </table>
        </div>
    </div>
    <div class="row-fluid">
      <div class="span4 offset4">
        <form>
            <fieldset>
              <input type="text" placeholder="Student NetID"><br />
              <button type="submit" class="btn-large"><span class="">Start Session</span></button>
            </fieldset>
        </form>
      </div>
    </div>